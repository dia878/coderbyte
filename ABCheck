def ABCheck(str)
	arr = str.split(//)
  	i = 0 

  while (i+4) < arr.length
    if arr[i] == "a" &&  arr[i+4] == "b"
      return "true"
    end
    i = i +1
  end
  return "false"
  # code goes here
         
end
   
# keep this function call here 
# to see how to enter arguments in Ruby scroll down   
ABCheck(STDIN.gets)  


